{
  "manifest_version": 3,
  "name": "kbdvid",
  "version": "0",
  "description": "Enable keyboard shortcuts for video",
  "icons": {
    "128": "off.png"
  },
  "permissions": [
    "tabs",
    "userScripts"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {}
}