{
  "manifest_version": 3,
  "name": "stepvf",
  "version": "0",
  "description": "Step video frame on keyboard ('<' or '>')",
  "icons": {
    "128": "off.png"
  },
  "permissions": [
    "contentSettings",
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {}
}